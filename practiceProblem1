#include<bits/stdc++.h>
using namespace std;
int countRemainingCandies(int n, int c, int a[])
{
    int totalCandies=0;
    for(int i=0;i<n;i++)
    {
        totalCandies = totalCandies + a[i];
    }
    int remainingCandies = totalCandies - (totalCandies/c)*c;
    return remainingCandies;
}
int main()
{
    int t;
    cin>>t;
    int tc = 1;
    while(t--)
    {
        int c;
        int n;
        cin>>n;//no of bags
        cin>>c;//no of children
        int *a = new int[n];
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
        }
        int ans = countRemainingCandies(n,c,a);
        cout << "Case #" << tc++ <<": " << ans << endl;
    
        delete[] a;
        
    }
    
}
